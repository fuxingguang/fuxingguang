<body>
    <script>
        var arr = [8,2,3,3,4,17,6,17,'',25];
			function duplicateRemoval(arr){
				for(var i = 0;i < arr.length;i++){
					for(var j = i + 1;j < arr.length;j++){
						if(arr[i]==arr[j]){
							arr.splice(j,1);
							j--;
						}
					}
				}
				return arr;
			}
    </script>
</body>
